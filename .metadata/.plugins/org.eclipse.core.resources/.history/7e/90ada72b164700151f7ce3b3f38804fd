public class HotSwapClassLoader extends ClassLoader{
	
	public HotSwapClassLoader()
	{
		super(HotSwapClassLoader.class.getClassLoader());
	}
	
	public Class loadByte(byte[] classByte)
	{
		return defineClass();
	}

